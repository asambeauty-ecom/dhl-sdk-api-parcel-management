{
  "name": "dhl/sdk-api-parcel-management",
  "description": "SDK for calling DHLs parcel management service API",
  "keywords": [
    "php",
    "sdk",
    "api",
    "DHL"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Ertner",
      "email": "sebastian.ertner@netresearch.de",
      "homepage": "https://www.netresearch.de/"
    },
    {
      "name": "Max Melzer",
      "email": "max.melzer@netresearch.de",
      "homepage": "https://www.netresearch.de/"
    }
  ],
  "require": {
    "php": "^7.0.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-message": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.0",
    "psr/log": "^1.1",
    "php-http/logger-plugin": "^1.0",
    "netresearch/jsonmapper": "^1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.0",
    "guzzlehttp/psr7": "^1.0",
    "php-http/mock-client": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Dhl\\ParcelManagement\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhl\\ParcelManagement\\Test\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0.x-dev"
    }
  }
}
